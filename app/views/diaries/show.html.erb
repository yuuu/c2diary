<h1>New Diary</h1>

<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col l8 s12">
    <table>
      <thead>
        <tr>
          <th>Date</th>
          <th>Body</th>
          <th>User</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <tr>
          <%= render partial: 'diary', locals: { diary: @diary } %>
        </tr>
      </tbody>
    </table>
</div>

