<h1>Show Diary</h1>

<div class="row">
  <div class="col l8 s12">
    <table>
      <thead>
        <tr>
          <th>Date</th>
          <th>Body</th>
          <th>User</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <tr>
          <%= render partial: 'diary', locals: { diary: @diary } %>
        </tr>
      </tbody>
    </table>
</div>
<div class="row">
  <div class="col l12">
    <%= link_to 'Back', diaries_path, class: 'back-btn' %>
  </div>
</div>

